<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

<% if(public.posts._data[path]) { %>
<meta property="og:title" content="<%- public.posts._data[path].title %>">
<title><%- public.posts._data[path].title %></title>
<meta property="og:description" content="<%- public.posts._data[path].description %>">
<meta property="og:type" content="article">
<% } else { %>
<meta property="og:title" content="<%- title %>">
<title><%- title %></title>
<% } %>
<meta property="og:image" content="https://www.heathanderson.net/favicon.png">

<link rel="manifest" href="site.webmanifest">
<link rel="apple-touch-icon" href="/favicon.png">

<link rel="stylesheet" href="/stylesheets/pure-min.css" />
